## The function is currently defined as
function (file, width = 7, height = 7, panels = NULL) 
{
    if (length(panels) == 2) {
        height = 5 * panels[1]
        width = 5 * panels[2]
    }
    postscript(file = file, horizontal = FALSE, onefile = FALSE, 
        paper = "special", width = width, height = height)
  }
}
